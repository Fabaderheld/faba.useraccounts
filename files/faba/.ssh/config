    HostName mali-srv.no-ip.info
    Port 22
    User copy
    IdentityFile /home/faba/.ssh/copy_rsa

host parents-copy
    HostName plex-parents.no-ip.info
    Port 2222
    User copy
    IdentityFile /home/faba/.ssh/copy_rsa


host admin.lits
   Hostname admin.lits.at
   Port 22
   User root
   IdentityFile /home/faba/.ssh/faba_rsa

host ollie
